{
  "name": "product_catalog_backend",
  "private": true,
  "version": "1.0.0",
  "homepage": "https://github.com/AleksandrTsivilin/online-store-phones-backend",
  "description": "",
  "type": "module",
  "main": "index.ts",
  "scripts": {
    "migrations-up": "npx sequelize db:migrate",
    "seeders-up": "npx sequelize db:seed:all",
    "migrations-seeders": "npm run migrations-up && npm run seeders-up",
    "format": "prettier --write . --ignore-path .gitignore",
    "lint": "eslint --ext .ts --ignore-path .gitignore .",
    "build": "swc src -d dist",
    "build-dev": "cross-env NODE_ENV=development npm run build && node --inspect dist/index.js",
    "dev": "npm run migrations-seeders && nodemon --ext ts --exec \"npm run build-dev\"",
    "start": "npm run build && cross-env NODE_ENV=production node dist/index.js",
    "prepare": "husky install",
    "sync-tables": "node dist/utils/syncTables.js"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.1",
    "pg": "^8.11.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.32.1",
    "sequelize-typescript": "^2.1.5"
  },
  "devDependencies": {
    "@swc-node/register": "^1.6.6",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.71",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.46.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "sequelize-cli": "^6.6.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AleksandrTsivilin/online-store-phones-backend.git"
  },
  "bugs": {
    "url": "https://github.com/AleksandrTsivilin/online-store-phones-backend/issues"
  },
  "lint-staged": {
    "*.{ts}": "npm run lint"
  }
}
